{
    "name": "vh-api",
    "version": "1.0.0",
    "description": "## API",
    "main": "app.js",
    "engines": {
        "node": "11.x"
    },
    "scripts": {
        "clean": "rm -rf build && mkdir build",
        "build-babel": "babel -d ./build ./src -s",
        "build": "npm run clean && npm run build-babel",
        "start": "npm run build && node ./build/app.js",
        "dev": "nodemon --exec babel-node ./src/app.js",
        "test": "mocha --require @babel/register --watch --watch-extensions js './test/**/*.js'"
    },
    "repository": {
        "type": "git",
        "url": "git+https://claudiomaxx@github.com/claudiomaxx/vh-api.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/claudiomaxx/vh-api/issues"
    },
    "homepage": "https://github.com/claudiomaxx/vh-api#readme",
    "dependencies": {
        "@babel/cli": "^7.4.3",
        "express": "^4.16.4"
    },
    "devDependencies": {
        "@babel/core": "^7.4.3",
        "@babel/node": "^7.2.2",
        "@babel/preset-env": "^7.4.3",
        "chai": "^4.2.0",
        "chai-as-promised": "^7.1.1",
        "chai-http": "^4.2.1",
        "mocha": "^6.1.4",
        "nodemon": "^1.18.11",
        "supertest": "^4.0.2"
    }
}
